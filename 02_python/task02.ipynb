   "source": [
    "tax_excluded_price = float(input(\"税抜価格を入力してください：\"))\n",
    "\n",
    "price = round(tax_excluded_price * 1.1)\n",
    "\n",
    "if price >= 2000:\n",
    "    print(\"送料は無料です\")\n",
    "else:\n",
    "    print(\"送料は350円です\")\n",
    "    price += 350\n",
    "\n",
    "print(f\"送料込税込価格は{price}円です。\")\n"
   ]
  }
